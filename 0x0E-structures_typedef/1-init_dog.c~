#include <stdlib.h>
#include "dog.h"
11;rgb:0000/0000/0000/**
 * init_dog - initialize a variable of type struct dog
 * @d: struct
 * @name: name
 * @age: age
 * @owner: owner
 */
void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d != NULL)
{
d = malloc(sizeof(struct dog));
d->name = name;
d->age = age;
d->owner = owner;
}
}
